{
  "name": "@nomaton/rollup-plugin-rusty-wasm",
  "version": "0.9.0",
  "description": "Rollup plugin to bundle wasm files generated by the Rust wasm-pack",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "test": "ava",
    "prepare": "npm run build",
    "build": "npm run clean && rollup -c",
    "clean": "node -e \"fs.rmSync('dist', {recursive:true, force:true})\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomaton/rollup-plugin-rusty-wasm.git"
  },
  "keywords": [
    "rollup-plugin",
    "wasm"
  ],
  "author": "nomaton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomaton/rollup-plugin-rusty-wasm/issues"
  },
  "homepage": "https://github.com/nomaton/rollup-plugin-rusty-wasm#readme",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "ava": "^4.1.0",
    "rollup": "^2.70.1",
    "rollup-plugin-ts": "^2.0.5",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}